{
  "name": "hailan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env VUE_APP_WORLD=serve vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "temp": "cross-env VUE_APP_WORLD=test vue-cli-service build && charon publish temp",
    "test": "cross-env VUE_APP_WORLD=test vue-cli-service build && charon publish test",
    "prod": "cross-env VUE_APP_WORLD=prod vue-cli-service build && charon publish prod"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.4.3",
    "normalize.css": "^8.0.1",
    "sentry": "^0.1.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.19.1",
    "sass": "^1.24.0",
    "sass-loader": "^8.0.0",
    "vconsole": "^3.3.4",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "globals": {
      "fiboSDK": false,
      "PALifeOpen": false,
      "$webfont": false,
      "PALifeWebSDK": false,
      "Sentry": false,
      "Wechat": false,
      "wx": false
    },
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "ios >= 8",
    "android >= 4",
    "ie >= 8",
    "Firefox >= 20",
    "Safari >= 5",
    "not dead",
    "not op_mini all"
  ],
  "publishPath": {
    "test": {
      "path": "./dist"
    },
    "temp": {
      "path": "./dist"
    },
    "prod": {
      "path": "./dist"
    }
  }
}
